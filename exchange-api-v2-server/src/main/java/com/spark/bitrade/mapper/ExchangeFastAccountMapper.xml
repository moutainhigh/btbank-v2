<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.spark.bitrade.mapper.ExchangeFastAccountMapper">
    <select id="findByAppIdAndCoinSymbol" resultType="com.spark.bitrade.entity.ExchangeFastAccount">
        select * from exchange_fast_account
        where app_id = #{appId}
        and coin_symbol = #{coinSymbol}
        and base_symbol = #{baseSymbol}
        and enable=1
    </select>

</mapper>